{
  "name": "epilogue-starter-kit",
  "version": "0.5.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/petekeller2/Epilogue-Starter-Kit.git"
  },
  "keywords": [
    "epilogue",
    "sequelize",
    "javascript",
    "nodejs"
  ],
  "description": "A starter kit using an Express implementation of Epilogue",
  "author": "Peter Keller <petekeller2@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/app.js --exec babel-node",
    "start-no-nodemon": "babel-node src/app.js",
    "build": "babel src -d build",
    "serve": "node build/app.js",
    "unit-test": "mocha test/unitTests --recursive --compilers js:babel-register --require babel-polyfill",
    "test-autoAssociations": "mocha test/unitTests/autoAssociations --recursive --compilers js:babel-register",
    "test-permissions": "mocha test/unitTests/permissions --recursive --compilers js:babel-register",
    "test-groups": "mocha test/unitTests/groups --recursive --compilers js:babel-register --require babel-polyfill",
    "test-failing": "mocha test/unitTests/failing.js --compilers js:babel-register",
    "http-test": "mocha test/httpTests --recursive --compilers js:babel-register",
    "http-just-aa-test": "mocha test/httpTests/autoAssociations/associations.js --compilers js:babel-register",
    "http-just-access-test": "mocha test/httpTests/permissions/access.js --compilers js:babel-register"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~3.1.0",
    "deepmerge": "^2.0.0",
    "epilogue": "^0.7.1",
    "express": "~4.16.2",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.6",
    "fs-extra": "^4.0.2",
    "jsonwebtoken": "^8.1.0",
    "mysql": "^2.15.0",
    "nodemailer": "^4.2.0",
    "passport": "^0.4.0",
    "passport-auth0": "^0.6.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.15.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "flow-bin": "^0.57.3",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.6.3",
    "jscpd": "^0.6.15",
    "jsdoc": "^3.5.5",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "pluralize": "^7.0.0",
    "request": "^2.83.0",
    "retire": "^1.4.0"
  }
}
