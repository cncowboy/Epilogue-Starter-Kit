{
  "name": "epilogue-starter-kit",
  "version": "0.02.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/petekeller2/Epilogue-Starter-Kit.git"
  },
  "keywords": [
    "epilogue",
    "sequelize",
    "javascript",
    "nodejs"
  ],
  "author": "Peter Keller <petekeller2@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/app.js --exec babel-node",
    "start-no-nodemon": "babel-node src/app.js",
    "build": "babel src -d build",
    "serve": "node build/app.js",
    "test": "mocha test/tests --recursive --compilers js:babel-register",
    "http-test": "mocha test/httpTests --recursive --compilers js:babel-register",
    "http-just-aa-test": "mocha test/httpTests/autoAssociations/associations.js --compilers js:babel-register",
    "http-just-access-test": "mocha test/httpTests/permissions/access.js --compilers js:babel-register"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.6",
    "deepmerge": "^1.3.2",
    "epilogue": "^0.7.1",
    "express": "~4.15.2",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.2",
    "jsonwebtoken": "^7.4.0",
    "mysql": "^2.13.0",
    "passport": "^0.3.2",
    "passport-auth0": "^0.6.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4",
    "fs-extra": "^3.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.6.3",
    "jscpd": "^0.6.11",
    "jsdoc": "^3.4.3",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "pluralize": "^4.0.0",
    "request": "^2.81.0",
    "retire": "^1.2.13"
  }
}
