{
  "name": "epilogue-starter-kit",
  "version": "0.5.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/petekeller2/Epilogue-Starter-Kit.git"
  },
  "keywords": [
    "epilogue",
    "sequelize",
    "javascript",
    "nodejs"
  ],
  "author": "Peter Keller <petekeller2@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/app.js --exec babel-node",
    "start-no-nodemon": "babel-node src/app.js",
    "build": "babel src -d build",
    "serve": "node build/app.js",
    "test": "mocha test/tests --recursive --compilers js:babel-register --require babel-polyfill",
    "test-autoAssociations": "mocha test/tests/autoAssociations --recursive --compilers js:babel-register",
    "test-permissions": "mocha test/tests/permissions --recursive --compilers js:babel-register",
    "test-groups": "mocha test/tests/groups --recursive --compilers js:babel-register --require babel-polyfill",
    "test-failing": "mocha test/tests/failing.js --compilers js:babel-register",
    "http-test": "mocha test/httpTests --recursive --compilers js:babel-register",
    "http-just-aa-test": "mocha test/httpTests/autoAssociations/associations.js --compilers js:babel-register",
    "http-just-access-test": "mocha test/httpTests/permissions/access.js --compilers js:babel-register"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.0",
    "cookie-parser": "~1.4.3",
    "debug": "~3.0.1",
    "deepmerge": "^1.5.1",
    "epilogue": "^0.7.1",
    "express": "~4.15.4",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.5",
    "fs-extra": "^4.0.1",
    "jsonwebtoken": "^8.0.0",
    "mysql": "^2.14.1",
    "passport": "^0.4.0",
    "passport-auth0": "^0.6.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.8.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.54.1",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.6.3",
    "jscpd": "^0.6.13",
    "jsdoc": "^3.5.4",
    "mocha": "^3.5.1",
    "nodemon": "^1.12.0",
    "pluralize": "^7.0.0",
    "request": "^2.81.0",
    "retire": "^1.4.0"
  }
}
